<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeSeekBarView_old">
        <attr name="seek_bg_color" format="color" />
        <attr name="seek_pb_color" format="color" />
        <attr name="seek_ball_solid_color" format="color" />
        <attr name="seek_ball_stroke_color" format="color" />
        <attr name="seek_text_color" format="color" />
        <attr name="seek_text_size" format="dimension" />
        <attr name="seek_vertical_line" format="boolean" />
        <attr name="seek_step_move" format="boolean" />
        <attr name="seek_mode" format="enum">
            <enum name="single" value="1" />
            <enum name="range" value="2" />
        </attr>
        <attr name="seek_step_length" format="float" />
    </declare-styleable>

    <style name="RangeSeekBarViewStyle">
        <item name="seek_bg_color">#D9D9D9</item>
        <item name="seek_pb_color">#FED854</item>
        <item name="seek_ball_solid_color">#FFFFFF</item>
        <item name="seek_ball_stroke_color">#FED854</item>
        <item name="seek_text_color">#959595</item>
        <item name="seek_text_size">14sp</item>
    </style>
</resources>